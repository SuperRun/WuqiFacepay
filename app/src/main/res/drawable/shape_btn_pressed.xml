<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="@dimen/md_login_btn_width" android:height="@dimen/md_login_btn_height">
        <shape  xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <solid android:color="#ffffffff" />
            <gradient  android:type="linear" android:useLevel="true" android:endColor="@color/btnEndColor" android:startColor="@color/btnStartColor" android:angle="180" />
            <corners android:topLeftRadius="120dp" android:topRightRadius="120dp" android:bottomLeftRadius="120dp" android:bottomRightRadius="120dp" />
        </shape>
    </item>
    <item android:width="@dimen/md_login_btn_icon_width" android:height="@dimen/md_login_btn_icon_height" android:gravity="center">
        <bitmap  android:src="@drawable/login_btn"/>
    </item>
</layer-list>
